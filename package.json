{
  "name": "azurite",
  "version": "3.0.0-preview",
  "description": "A lightweight server clone of Azure Storage that simulates most of the commands supported by it with minimal dependencies.",
  "bin": {
    "azurite": "./dist/src/blob/bin.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@azure/ms-rest-js": "^1.5.0",
    "express": "^4.16.4",
    "lokijs": "^1.5.6",
    "morgan": "^1.9.1",
    "tslib": "^1.9.3",
    "uri-templates": "^0.2.0",
    "winston": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@azure/storage-blob": "^10.3.0",
    "@types/express": "^4.16.0",
    "@types/lokijs": "^1.5.2",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/uri-templates": "^0.1.29",
    "@types/xml2js": "^0.4.3",
    "autorest": "^2.0.4283",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.1.4"
  },
  "scripts": {
    "build": "tsc",
    "build:autorest": "autorest ./swagger/blob.md --typescript --use=E:/GitHub/XiaoningLiu/autorest.typescript.server",
    "build:autorestdebug": "autorest ./swagger/blob.md --typescript --typescript.debugger --use=E:/GitHub/XiaoningLiu/autorest.typescript.server",
    "blob": "node -r ts-node/register src/blob/bin.ts",
    "test": "mocha --compilers ts-node/register --no-timeouts tests/**/*.test.ts",
    "clean": "rimraf dist typings *.log coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azure/azurite.git"
  },
  "keywords": [
    "Azurite",
    "Azure",
    "Storage",
    "Blob",
    "Queue",
    "Emulator",
    "Microsoft"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azure/azurite/issues"
  },
  "homepage": "https://github.com/azure/azurite#readme"
}