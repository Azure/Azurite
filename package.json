{
  "name": "azurite",
  "version": "2.6.6",
  "description": "A lightweight server clone of Azure Blob, Queue, and Table Storage that simulates most of the commands supported by it with minimal dependencies.",
  "scripts": {
    "pretest": "npm run clean",
    "test": "env-cmd ./test/.env jest --runInBand --detectOpenHandles ",
    "test:watch": "env-cmd ./test/.env jest --watch",
    "externaltests": "env-cmd ./externaltests/.env mocha -r ts-node/register externaltests/azure-storage-shim.js",
    "start": "ts-node bin/azurite -l azurite-testdrive",
    "blob": "ts-node bin/blob.js -l azurite-testdrive",
    "queue": "ts-node bin/queue.js",
    "table": "ts-node bin/table.js",
    "clean": "rimraf azurite-testdrive *.nupkg blob.exe queue.exe table.exe",
    "nuget": "cross-var \"npm run clean && pkg -t node6-win --output blob bin/blob && pkg -t node6-win --output queue bin/queue && pkg -t node6-win --output table bin/table && nuget pack -Version $npm_package_version && nuget push *.nupkg  -Source https://www.nuget.org/api/v2/package\"",
    "docker": "cross-var \"docker build -t arafato/azurite:$npm_package_version . && docker build -t arafato/azurite:latest . && docker push arafato/azurite:$npm_package_version && docker push arafato/azurite:latest\""
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "main": "./bin/azurite.js",
  "bin": {
    "azurite": "./bin/azurite.js",
    "azurite-blob": "./bin/blob.js",
    "azurite-queue": "./bin/queue.js",
    "azurite-table": "./bin/table.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arafato/azurite.git"
  },
  "keywords": [
    "azure",
    "storage",
    "blob",
    "queue",
    "table",
    "blobstore",
    "emulator",
    "microsoft"
  ],
  "author": "Oliver Arafat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arafato/azurite/issues"
  },
  "homepage": "https://github.com/arafato/azurite#readme",
  "dependencies": {
    "@types/node": "^10.5.5",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "combined-stream": "^1.0.5",
    "express": "^4.14.0",
    "fs-extra": "^4.0.1",
    "js2xmlparser": "^2.0.2",
    "lokijs": "^1.4.1",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "request": "2.87.0",
    "typescript": "^3.0.3",
    "uuid": "^3.0.1",
    "validator": "^8.0.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.23",
    "@types/chai": "^4.1.4",
    "@types/chai-http": "^3.0.5",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.1",
    "@types/mocha": "^5.2.5",
    "@types/request-promise": "^4.1.42",
    "@types/xml2js": "^0.4.3",
    "azure-storage": "^2.10.1",
    "chai": "^3.5.0",
    "chai-http": "^4.0.0",
    "cross-var": "^1.1.0",
    "env-cmd": "^8.0.2",
    "jest": "^23.5.0",
    "mocha": "5.2.0",
    "pkg": "^4.3.1",
    "prettier": "1.14.2",
    "request-promise": "^4.1.1",
    "rimraf": "2.6.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "ts-jest"
    },
    "testRegex": "_Tests\\.js$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  }
}
